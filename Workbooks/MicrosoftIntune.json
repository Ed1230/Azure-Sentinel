      {
        "type": 3,
        "content": {
          "version": "KqlItem/1.0",
          "query": "IntuneOperationalLogs\n| where OperationName == \"Enrollment\"\n| extend EnrollmentTimeUTC_ = tostring(parse_json(Properties).EnrollmentTimeUTC) \n| extend EnrollmentType_ = tostring(parse_json(Properties).EnrollmentType) \n| extend FailureCategory_ = tostring(parse_json(Properties).FailureCategory) \n| extend FailureReason_ = tostring(parse_json(Properties).FailureReason) \n| extend Os_ = tostring(parse_json(Properties).Os) \n| extend OsVersion_ = tostring(parse_json(Properties).OsVersion) \n| project EnrollmentTimeUTC_, EnrollmentType_, Os_, OsVersion_, FailureCategory_, FailureReason_",
